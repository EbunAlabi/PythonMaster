
#notice how the list gets populated
my_list = list(range(10))
print(my_list)

even = list(range(0,10,2))
odd = list(range(1,10,2))
print(even)
print(odd)

decimals = range (0,100)
print(decimals)

#different range formatting styles
my_range = decimals [3:40:3]
print(my_range)

for i in my_range:
    print(i)

print('=' * 40)

for i in range(3,40,3):
    print(i)

#challenge
o = range (0,100,5)
print(o)
p = o[::5]
print (p)
for i in p :
    print(i)

